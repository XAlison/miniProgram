}


.radio-group{
justify-content: space-around;
font-size:32rpx; 
  display: flex;
border-bottom: 2rpx solid #ddd; 
border-top: 2rpx solid #ddd; 
padding: 12rpx;
}
.radio-icon{
  width: 40rpx;
  height: 40rpx;
  position: relative;
  top: 6rpx;
  right: 8rpx;
  border-radius: 50%;
}
.status-radio{
  display: none;
}
