.process-list{
  font-size: 28rpx;
  padding: 8rpx 24rpx;
  flex-wrap: wrap;
  justify-content: flex-start;
  color: #1498F7;
}
.process-icon{
  position: relative;
 top: 4rpx;
  margin-right: 8rpx;
}
.process-item{
  margin-right: 12rpx;
